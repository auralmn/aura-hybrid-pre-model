        "leaky_integrate_fire": {
          "name": "Leaky Integrate-and-Fire (LIF)",
          "equations": "τ_m*dV/dt = -(V-V_rest) + R*I; if V ≥ V_thresh then V = V_reset",
          "firing_patterns": {
