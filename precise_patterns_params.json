              "units": "ms, mV, mV, mV, MΩ",
              "current_range": "I > 1.6 nA (rheobase)",
              "firing_rate": "F = 1/[τ_ref + τ_m*ln(R*I/(R*I - (V_thresh - V_rest)))]",
              "characteristics": "Perfect integrator, constant threshold"
